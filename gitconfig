[init]
  defaultBranch = master
  templatedir = ~/.git_template
[push]
  default = current
[color]
  ui = auto
[core]
  excludesfile = ~/.gitignore
  autocrlf = input
[merge]
  ff = only
[fetch]
  prune = true
[rebase]
  autosquash = true
[include]
  path = ~/.gitconfig.local
[diff]
  colorMoved = zebra
